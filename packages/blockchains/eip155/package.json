    "@typechain/ethers-v5": "^10.0.0",
    "ethers": "^5.6.0",
    "hardhat": "^2.6.5",
    "hardhat-deploy": "^0.9.3",
    "hardhat-deploy-ethers": "^0.3.0-beta.11",
    "typechain": "^8.0.0"
  },
  "peerDependencies": {
    "ethers": "^5.6.0"
  },
