{
  "name": "@iqprotocol/eip155",
  "version": "0.15.7",
  "license": "UNLICENSED",
  "scripts": {
    "start": "rollup -cw",
    "build": "rollup -c",
    "test": "jest --group=unit",
    "test:integration": "jest --group=integration",
    "prepublish": "run-s typechain build",
    "typechain": "run-s typechain:clean typechain:generate",
    "typechain:clean": "rimraf src/contracts",
    "typechain:generate": "typechain --target ethers-v5 --out-dir src/contracts './node_modules/@iqprotocol/solidity-contracts/artifacts/contracts/**/*.json'"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "typedocMain": "src/index.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "@iqprotocol/abstract-blockchain": "^0.10.2"
  },
  "devDependencies": {
    "@iqprotocol/solidity-contracts": "https://github.com/iqalliance/iq-smart-contracts#b88ecf3059f893e2da42840a8d9fca6c8e96d04e",
    "@typechain/ethers-v5": "^7.0.1",
    "ethers": "^5.4.5",
    "hardhat": "^2.6.1",
    "hardhat-deploy": "^0.9.0",
    "hardhat-deploy-ethers": "^0.3.0-beta.10",
    "typechain": "^5.1.2"
  },
  "peerDependencies": {
    "ethers": "^5.4.5"
  }
}
